{
  "name": "kosuha606/virtual-model-helppack",
  "description": "VirtualModelHelppack",
  "type": "library",
  "authors": [
    {
      "name": "Eugene Kosuhin",
      "email": "kosuha606@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0.0",
    "kosuha606/virtual-model": "^1.2.5",
    "php-di/php-di": "^6.2.1"
  },
  "autoload": {
    "psr-4": {
      "kosuha606\\VirtualModelHelppack\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "6.5",
    "phpstan/phpstan": "^0.12.81"
  },
  "scripts": {
    "check": [
      "@stan",
      "@ut"
    ],
    "ut": "phpunit --configuration C:\\OpenServer\\domains\\virtual-model-helppack\\phpunit.xml",
    "stan": "vendor\\bin\\phpstan analyse -l 3 C:\\OpenServer\\domains\\virtual-model-helppack\\src C:\\OpenServer\\domains\\virtual-model-helppack\\tests"
  }
}
